{
  "name": "api.arthcoin.com",
  "version": "1.0.2",
  "author": "Vassilis Mastorostergios <vmasto@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf dist",
    "build": "npm run clean && tsc",
    "test": "jest --watch",
    "lint": "esw -w src test"
  },
  "keywords": [
    "express",
    "babel",
    "boilerplate",
    "scaffold",
    "es6",
    "es2015",
    "es2016",
    "es2017",
    "jest",
    "eslint"
  ],
  "dependencies": {
    "@ethersproject/address": "^5.6.1",
    "@ethersproject/solidity": "^5.6.1",
    "@moralisweb3/evm": "^2.0.0-alpha.2",
    "@moralisweb3/evm-utils": "^2.0.1",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@truffle/hdwallet-provider": "^1.4.1",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.0",
    "@uniswap/sdk": "^3.0.3",
    "@uniswap/sdk-core": "^3.1.0",
    "@uniswap/smart-order-router": "^2.10.2",
    "@uniswap/v3-sdk": "^3.9.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.18.2",
    "chai": "^4.3.7",
    "cheerio": "^1.0.0-rc.10",
    "chunk": "^0.0.3",
    "coingecko-api": "^1.0.10",
    "cors": "^2.8.5",
    "ethcall": "^4.4.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.0.0",
    "express": "^4.16.4",
    "google-spreadsheet": "^3.3.0",
    "hardhat": "^2.12.2",
    "mongoose": "^6.5.1",
    "mongoose-timestamp": "^0.6.0",
    "moralis": "^2.1.1",
    "morgan": "^1.9.1",
    "nconf": "^0.11.3",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.0",
    "node-fetch": "^3.2.3",
    "pug": "^2.0.3",
    "reddit": "^1.2.1",
    "request-promise": "^4.2.6",
    "rimraf": "^3.0.2",
    "underscore": "^1.13.4",
    "web3": "^1.4.0"
  },
  "devDependencies": {
    "@types/coingecko-api": "^1.0.0",
    "@types/nconf": "^0.10.0",
    "@types/node": "^18.11.9",
    "@types/node-cron": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-register": "^6.26.0",
    "dotenv": "^10.0.0",
    "eslint": "^5.15.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-watch": "^5.0.1",
    "jest": "^24.5.0",
    "nodemon": "^1.18.10",
    "supertest": "^4.0.2",
    "ts-node": "^10.1.0",
    "tsc": "^2.0.3",
    "typescript": "^4.3.5"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "plugins": [
      "import",
      "jest"
    ],
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "rules": {
      "jest/no-focused-tests": 2,
      "jest/no-identical-title": 2
    }
  },
  "jest": {
    "testEnvironment": "node"
  }
}
